 */

import stanford.karel.*;

public class randomizer extends SuperKarel {
	
	public void run() {
		for (int i = 0; i < 50; i++) {
			if (random())
				turnLeft();
			if (random())
				turnAround();
			if (random()) {
				if (beepersPresent())
					pickBeeper();
			}
			else if (random())
				putBeeper();
			randMove(3);
		}
	}
	
	private void randMove(int times) {
		for (int i = 0; i < times; i++) {
			if (random())
				move();
		}
	}
}